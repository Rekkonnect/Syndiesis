<UserControl
  x:Class="Syndiesis.Controls.Editor.BackgroundLineNumberPanel"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:aediting="using:AvaloniaEdit.Editing"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  HorizontalAlignment="Stretch"
  VerticalAlignment="Stretch"
  d:DesignHeight="250"
  d:DesignWidth="60"
  mc:Ignorable="d"
  >

  <Panel
    Name="panel"
    >
    <Rectangle
      Fill="#8000333E"
      />
    <aediting:LineNumberMargin
      Name="lines"
      Margin="30 0 0 0"
      />
  </Panel>
</UserControl>
