<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignWidth="550"
             d:DesignHeight="28"
             MinWidth="550"
             Height="28"
             xmlns:tabs="using:Syndiesis.Controls.Tabs"
             x:Class="Syndiesis.Controls.AnalysisVisualization.AnalysisViewTabRow">

  <Panel>

    <tabs:TabEnvelopeRow
      Name="analysisTreeViewTabs"
      HorizontalAlignment="Left"
    />

    <tabs:TabEnvelopeRow
      Name="analysisAlternateViewTabs"
      HorizontalAlignment="Right"
      Padding="0 0 25 0"
    />

  </Panel>

</UserControl>
