<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             xmlns:controls="using:CSharpSyntaxEditor.Controls"
             d:DesignWidth="800" d:DesignHeight="450"
             x:Class="CSharpSyntaxEditor.Controls.CodeEditorContentPanel">

  <StackPanel Name="codeLinesPanel">
    <controls:CodeEditorLine
      >
      <controls:CodeEditorLine.Inlines>
        <Run>using System;</Run>
      </controls:CodeEditorLine.Inlines>
      
    </controls:CodeEditorLine>
    <controls:CodeEditorLine
      >
      <controls:CodeEditorLine.Inlines>
      </controls:CodeEditorLine.Inlines>
    </controls:CodeEditorLine>
    <controls:CodeEditorLine
      >
      <controls:CodeEditorLine.Inlines>
        <Run>namespace Example;</Run>
      </controls:CodeEditorLine.Inlines>
    </controls:CodeEditorLine>
    <controls:CodeEditorLine
      >
      <controls:CodeEditorLine.Inlines>
      </controls:CodeEditorLine.Inlines>
    </controls:CodeEditorLine>
    <controls:CodeEditorLine
      >
      <controls:CodeEditorLine.Inlines>
        <Run xml:space="preserve">Console.WriteLine("Initializing application...");</Run>
      </controls:CodeEditorLine.Inlines>
    </controls:CodeEditorLine>
  </StackPanel>
</UserControl>
